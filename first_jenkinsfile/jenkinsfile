  agent none
  stages {
    stage("Directory Creation") {
      steps {
        sh 'cd /home/ubuntu && mkdir gitdownload && cd gitdownload'
      }
      agent {
        docker {image 'maven:3.8.1-adoptopenjdk-11' }
      }
    }
  }
}



